<%% form_for [:admin, @<%= singular_name %>] do |f| %>
  <%%= f.error_messages %>
<%- for attribute in attributes -%>
  <p>
    <%%= f.label :<%= attribute.name %> %><br />
    <%%= f.<%= attribute.field_type %> :<%= attribute.name %> %>
  </p>
<%- end -%>
  <p><%%= f.submit "Submit" %> &nbsp;or&nbsp; <%%= link_to 'Cancel', admin_<%= plural_name %>_path %></p>
<%% end %>
